{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@pages": ["./src/pages/"],
      "@components": ["./src/components/"],
      "@statics": ["./src/statics/"],
      "@constants": ["./src/constants/"],
      "@services": ["./src/services/"],
      "@utils": ["./src/utils/"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
